{
  "compilerOptions":{
    "outDir": "dist",
    "module": "esnext",
    "target":"es2015",
    "lib": [
      "es6",
      "dom",
      "es2016",
      "es2017"
    ],
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "baseUrl": "build",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "allowJs": true,
    "jsx": "react",
    "noEmit": false,
    "strictNullChecks":true
  }
}
